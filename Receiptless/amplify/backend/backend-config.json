{
	"function": {
		"UploadToBucket": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		},
		"LambdaOCRS3": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		},
		"getReceiptsDB": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		},
		"deleteReceipt": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "s3bucket",
